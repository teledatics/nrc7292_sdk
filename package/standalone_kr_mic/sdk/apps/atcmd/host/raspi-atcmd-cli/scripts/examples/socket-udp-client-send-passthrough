ECHO "Run UDP server. (IPv4)"
ECHO " - IP : 192.168.200.1"
ECHO " - Port : 50000"
HOLD

AT
WAIT 1s

AT+SCLOSE
AT+SOPEN="udp",60000
AT+SLIST?

ECHO "Send data to server. (length: 1024-byte, count: 1024)"
HOLD

AT+SSEND=0,"192.168.200.1",50000,0
LOOP 1 1024
DATA 1024

WAIT 5s
AT

WAIT 2s
AT+SCLOSE
AT+SLIST?
